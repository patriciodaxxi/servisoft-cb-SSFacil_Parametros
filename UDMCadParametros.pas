procedure TDMCadParametros.prc_Gravar;
begin
  vMsgErro := '';
  if trim(cdsParametrosENDXMLNFE.AsString) = '' then
    vMsgErro := vMsgErro + #13 + '*** Endereo para salvar o XML no foi informado!';
  if trim(cdsParametrosENDPDFNFE.AsString) = '' then
    vMsgErro := vMsgErro + #13 + '*** Endereo para salvar o PDF no foi informado!';
  if trim(cdsParametrosVERSAOEMISSAONFE.AsString) = '' then
    vMsgErro := vMsgErro + #13 + '*** Verso da emisso da NFe no foi informado!';
  if trim(cdsParametrosVERSAONFE.AsString) = '' then
    vMsgErro := vMsgErro + #13 + '*** Verso da NFe no foi informado!';
  if trim(cdsParametrosLOCALSERVIDORNFE.AsString) = '' then
    vMsgErro := vMsgErro + #13 + '*** Local do servidor da NFe no foi informado!';
  if trim(cdsParametrosSERIENORMAL.AsString) = '' then
    vMsgErro := vMsgErro + #13 + '*** Srie para as notas no foi informado!';
  if trim(cdsParametrosSERIECONTINGENCIA.AsString) = '' then
    vMsgErro := vMsgErro + #13 + '*** Srie para as notas de contingncia no foi informado!';
  if vMsgErro <> '' then
    exit;
  cdsParametros.Post;
  cdsParametros.ApplyUpdates(0);
end;

